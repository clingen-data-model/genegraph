            :ProbandEvidenceLine]}}

 :interfaces
 {:Resource
  {:description "An RDF Resource; generic type suitable for return when a variety of resources may be returned as the result of a function all"
   :fields {:iri {:type String
                  :description "The IRI for this resource."}
            :curie {:type String
                    :description "The CURIE internal to Genegraph for this resource."}
            :label {:type String
                    :description "The label for this resouce."}}}

  :Proposition
  {:description "An abstract entity representing the sharable meaning of what is expressed in a particular assertion."
   :fields {:iri {:description "IRI of this proposition"
                  :type String}
            :curie {:description "CURIE of this proposition"
                    :type String}
            :type {:description "Type of this proposition"
                   :type (list :Class)}
            :subject {:description "A relation that holds between a statement or proposition which can be captured in part or in whole as a directional relationship between two entities, and the entity that representing the subject of this relationship."
                      :type :Resource}
            :predicate {:description "A relation that holds between a statement or proposition which can be captured in part or in whole as a directional relationship between two entities, and the property serving as the predicate in this relationship."
                        :type :Resource}
            :object {:description "A relation that holds between a statement or proposition which can be captured in part or in whole as a directional relationship between two entities, and the entity that representing the object or value of this relationship."
                     :type :Resource}
            :subject_of {:description "Entities that have this proposition as their subject"
                         :type (list :Assertion)}}}
  
  :Assertion
  {:description "A statement made by a particular agent on a particular occasion that a particular proposition is true, based on the evaluation of one or more lines of evidence."
   :fields {:subject {:description "A relation that holds between a statement or proposition which can be captured in part or in whole as a directional relationship between two entities, and the entity that representing the subject of this relationship."
                      :type :Resource}
            :predicate {:description "A relation that holds between a statement or proposition which can be captured in part or in whole as a directional relationship between two entities, and the property serving as the predicate in this relationship."
                        :type :Resource}
            :object {:description "A relation that holds between a statement or proposition which can be captured in part or in whole as a directional relationship between two entities, and the entity that representing the object or value of this relationship."
                     :type :Resource}
            :evidence_lines {:description "Evidence lines contributing to this assertion"
                             :type (list :EvidenceLine)}
            :iri {:description "IRI of this assertion"
                  :type String}
            :curie {:descirption "CURIE of this assertion"
                    :type String}
            :type {:description "Type of this assertion"
                   :type (list :Class)}
            :description {:descripiton "Description of this assertion"
                          :type String}}}

  :EvidenceLine
  {:description "A SEPIO evidence line"
   :fields {:evidence_items {:description "Evidence items in support of a given evidence line"
                             :type (list :EvidenceItem)}
            :iri {:description "IRI of this evidence line"
                  :type String}
            :type {:description "Type of this evidence line"
                   :type (list :Class)}
            :score {:description "Numeric score of this evidence line; usually exists within
the context of a formal evalutation framework"
                    :type Float}
            :description {:descripiton "Usually provides the reasoning behind the contribution of a given evidence item to a proposition or assertion"
                          :type String}}}
  
  :EvidenceItem
  {:description "A SEPIO evidence item"
   :fields {:evidence_lines {:description "Evidence lines supporting the assertion"
                             :type (list :EvidenceLine)
                             :args {:recursive {:description "Recurse through SEPIO has-evidence-line, has-evidence-item associations to find deeper evidence lines"
                                                :type Boolean}
                                    :class {:description "When recursive is true, filter selection to instances of a given class. Include subclass hierarchy."
                                            :type :SEPIOClass}}}
            :iri {:description "IRI of this evidence item"
                  :type String}
            :type {:description "Type of this evidence item"
                   :type (list :Class)}
            :description {:descripiton "A description of the evidence item"
                          :type String}}}

  :GenomicCoordinate
  {:description "Genomic coordinate of a Gene or Region"
   :fields {:build {:type String
                    :description "The build name"}
            :assembly {:type String
                       :description "The assembly name"}
            :chromosome {:type String
                         :description "The chromosome name"}
            :start_pos {:type Int
                        :description "Start coordinate"}
            :end_pos {:type Int
                      :description "End coordinate"}
            :strand {:type String
                     :description "Strand the feature appears on."}}}

  :GenomicFeature
  {:description "Genomic feature represented by a Gene or Region"
   :fields {:coordinates {:type (list :GenomicCoordinate)}}}

  :Curation
  {:fields {:report_date {:type String}}}}

 :objects
 {:Contribution
  {:description "A container describing an act of contribution to a piece of data"
   :fields {:iri {:type String
                  :resolve :resource/iri
                  :description "IRI of the Contribution record. Value is not persistent, subject to change."}
            :agent {:type String
                    :resolve :contribution/agent
                    :description "Reference to the agent who made the contribution"}
            :agent_role {:type String
                         :resolve :contribution/agent-role
                         :description "The role with which the agent made the contribution (author, evaluator, transformer, etc). Similar to 'contribution type' but from the agent's perspective."}
            :activity_date {:type String
                            :resolve :contribution/activity-date
                            :description "When the contribution was made."}}}

  :Variant
  {:description "An instance of variation in a genomic sequence."
   :fields {:iri {:type String
                  :resolve :resource/iri
                  :description "IRI of the variant record."}
            :id {:type String
                 :resolve :variant/id
                 :description "Persistent identifier for the piece of knowledge (http://purl.org/dc/terms/isVersionOf)"}
            :name {:type String
                   :resolve :variant/name
                   :description "The preferred label of the variant. Not necessarily a stable identifier."}
            :genes {:type (list :ClinVarGene)
                    :resolve :variant/genes
                    :description "Genes in which this variant occurs."}}}

  :ClinicalAssertion
  {:description "A single assertion about clinical or phenotypic significance of a genomic feature."
   :fields {:iri {:type String
                  :resolve :resource/iri
                  :description "IRI of the assertion. Scheme based on where the assertion was originally submitted."}
            :id {:type String
                 :resolve :clinvar/clinical-assertion-version-of
                 :description "Persistent identifier for the piece of knowledge (http://purl.org/dc/terms/isVersionOf)"}
            :version {:type String
                      :resolve :clinvar/clinical-assertion-version
                      :description "Version of the record. Record owner may increment when a change within the record was significant enough to change the meaning."}
            :subject {:type :Variant
                      :resolve :clinvar/clinical-assertion-subject
                      :description "Subject of the assertion. For variant assertions this will be a variant."}
            :predicate {:type String
                        :resolve :clinvar/clinical-assertion-predicate
                        :description "Predicate of the assertion. What is being asserted about the subject."}
            :object {:type String
                     :resolve :clinvar/clinical-assertion-object
                     :description "Object of the assertion. What the subject has by relation of the predicate."}
            :contribution {:type :Contribution
                           :resolve :clinvar/clinical-assertion-contribution
                           :description "Node holding information about a contribution to the record."}
            :review_status {:type String
                            :resolve :clinvar/clinical-assertion-review-status
                            :description "ClinVar review status field. Interpretable as the inferred confidence in the assertion by the knowledge base, not the assertion creator."}
            :release_date {:type String
                           :resolve :clinvar/clinical-assertion-release-date
                           :description "Date upstream knowledge base validated the submitted data. (Date ClinVar incorporated into a release.)"}
            :date_updated {:type String
                           :resolve :clinvar/clinical-assertion-date-updated
                           :description "Date the record was last updated. This is a record level timestamp, not about the assertion, but the record as it exists in the knowledge base."}
            :allele_origins {:type (list String)
                             :resolve :clinvar/clinical-assertion-allele-origin
                             :description "Assertion submitter specified origin for their observation of the variation."}
            :collection_methods {:type (list String)
                                 :resolve :clinvar/clinical-assertion-collection-method
                                 :description "Assertion submitted specified method used to collect the information included in the assertion."}
            :classification_context {:type String
                                     :resolve :clinvar/clinical-assertion-classification-context
                                     :description "ClinGen Classification Context"}
            }}



  :AggregateVariantAssertion
  {:description "An aggregation of assertion information about a variation"
   :fields {:iri {:type String
                  :resolve :resource/iri
                  :description "Unique identifier for a version of the piece of knowledge."}
            :id {:type String
                 :resolve :clinvar/aggregate-assertion-version-of
                 :description "Persistent identifier for the piece of knowledge, not a version of it."}
            :version {:type String
                      :resolve :clinvar/aggregate-assertion-version
                      :description "Data version of the record. Values within the record may change without the version changing."}
            :release_date {:type String
                           :resolve :clinvar/aggregate-assertion-release-date
                           :description "Date upstream knowledge base validated the submitted data. (Date ClinVar incorporated into a release.)"}
            :review_status {:type String
                            :resolve :clinvar/aggregate-assertion-review-status
                            :description "ClinVar review status field. Interpretable as the inferred confidence in the assertion by the knowledge base, not the assertion creator."}
            :subject {:type :Variant
                      :resolve :clinvar/aggregate-assertion-subject
                      :description "Subject of the assertion. For variant assertions this will be a variant."}
            :predicate {:type String
                        :resolve :clinvar/aggregate-assertion-predicate
                        :description "Predicate of the aggregate assertion."}
            :object {:type String
                     :resolve :clinvar/aggregate-assertion-object
                     :description "Object of the aggregate assertion. Given arbitrary objects of member assertions, the aggregate object may not be useful."}
            :members {:type (list :ClinicalAssertion)
                      :resolve :clinvar/aggregate-assertion-members
                      :description "List of member assertions of the aggregate assertion."}
            }}

  :ClinVarGene
  {:description "A Gene, but within the context of ClinVar Curation efforts."
   :fields {:iri {:type String
                  :resolve :resource/iri}
            :name {:type String
                   :resolve :clinvar/gene-preferred-label}
            :symbol {:type String
                     :resolve :clinvar/gene-symbol}}
   }

  :Gene
  {:description "A genomic feature (a gene or a region). Along with conditions, one of the basic units of curation."
   :implements [:Resource]
   :fields {:iri {:type String
                  :resolve :resource/iri
                  :description "IRI representing the gene. Uses NCBI gene identifiers"}
            :curie {:type String
                    :resolve :resource/curie
                    :description "CURIE of the IRI representing this resource."}
            :label {:type String
                    :resolve :resource/label
                    :description "Gene symbol"}
            :type {:type (list :Class)
                   :resolve :resource/type
                   :description "Type of the gene, includes gene and more specific classifications of the locus"}
            :alternative_label {:type String
                                :resolve :resource/alternative-label
                                :description "Full name of gene"}
            :last_curated_date {:type String
                                :resolve :gene/last-curated-date
                                :description "Most recent date a curation (of any kind) has been performed on this gene."}
            :chromosome_band {:type String
                              :resolve :gene/chromosome-band
                              :description "Cytogenetic band of gene."}
            :hgnc_id {:type String
                      :resolve :gene/hgnc-id
                      :description "HGNC ID of gene"}
            :curation_activities {:type (list :CurationActivity)
                                  :description "The curation activities that have published reports on the gene"
                                  :resolve :gene/curation-activities}
            :genetic_conditions {:type (list :GeneticCondition)
                                 :resolve :gene/conditions
                                 :description "Genetic conditions associated with gene. These represent gene-disease pairs for which a curation exists associated with the given gene."}
            :gene_validity_assertions {:type (list :GeneValidityAssertion)
                                       :resolve :gene/gene-validity-assertions
                                       :description "Gene validity assertions for a given gene"}
            :dosage_curation {:type :GeneDosageCuration
                              :resolve :gene/dosage-curation
                              :description "Gene Dosage curation associated with the gene or region."}}}

  :Genes
  {:description "A collection of genes."
   :fields {:gene_list {:type (list :Gene)}
            :count {:type Int}}}

  :GeneticCondition
  {:description "A condition described by some combination of gene, disease, and mode of inheritance (usually at least gene and disease)."
   :fields {:gene {:type :Gene
                   :resolve :genetic-condition/gene
                   :description "The gene associated with this genetic condition."}
            :disease {:type :Disease
                      :resolve :genetic-condition/disease
                      :description "The disease associated with this genetic condition."}
            :mode_of_inheritance {:type :ModeOfInheritance
                                  :resolve :genetic-condition/mode-of-inheritance
                                  :description "The mode of inheritance associated with this genetic condition."}
            :actionability_assertions {:type (list :ActionabilityAssertion)
                                       :resolve :genetic-condition/actionability-assertions
                                       :description "Actionability assertions associated with this genetic condition. Unlike gene_validity and dosage, there may be more than one per genetic condition, as the condition may be curated in both pediatric and adult contexts."}
            :actionability_curations {:type (list :ActionabilityCuration)
                                      :resolve :genetic-condition/actionability-curations
                                      :deprecated "use actionability_assertions field instead"
                                      :description "Actionability curations associated with this genetic condition. Unlike gene_validity and dosage, there may be more than one per genetic condition, as the condition may be curated in both pediatric and adult contexts."}
            :gene_validity_assertions {:type (list :GeneValidityAssertion)
                                       :resolve :genetic-condition/gene-validity-curation
                                       :description "Gene Validity curation associated with this genetic condition."}
            :gene_dosage_assertions {:type (list :DosageAssertion)
                                     :resolve :genetic-condition/gene-dosage-curation
                                     :description "Dosage sensitivity curation associated with this genetic condition."}}}

  :Coordinate
