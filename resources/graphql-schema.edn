   :values [:ALL :ACTIONABILITY :GENE_VALIDITY :GENE_DOSAGE]}
  :Role
  {:description "Role an agent realizes in the context of a qualified contribution"
   :values [:ANY :APPROVER :SECONDARY_CONTRIBUTOR]}
  :GeneDosageScore
